{% extends 'base.html.twig' %}

{% block body %}
    <pre>
    {{ dump(types) }}
    </pre>

    {% for type in types %}

        <pre>
            {{ dump(type) }}
        </pre>

        {% for evenement in type.evenements %}

            <pre>
                {{ dump(evenement) }}
            </pre>

        {% endfor %}

    {% endfor %}

{% endblock %}
